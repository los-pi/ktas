{
  "name": "ktas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-scripts": "4.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test-ut": "react-scripts test --runner=groups --group=unit ",
    "test-co": "react-scripts test --coverage --watchAll=false",
    "test-ci": "react-scripts test --runner=groups --group=unit --watchAll=false --coverage --testResultsProcessor=jest-junit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test:generate-output": "react-scripts test --json --outputFile=src/.jest-test-results.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-backgrounds": "^6.1.18",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-jest": "^6.1.18",
    "@storybook/addon-knobs": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addon-storyshots": "^6.1.18",
    "@storybook/node-logger": "^6.1.18",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.18",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "jest-junit": "^12.0.0",
    "jest-runner-groups": "^2.0.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}
